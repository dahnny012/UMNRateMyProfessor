{
	"manifest_version": 2,
  "web_accessible_resources": ["icon.png","conflict.png"],
  "name": "Rate my UMN professor",
  "version": "2.0",
  "description": "Brings up UMN Professor Ratings and informs of time conflicts",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup":"popup.html"
  },
  "permissions": [
    "activeTab","contextMenus"
  ],
  "background": {
    "scripts": ["context.js"],
    "persistent": true
  },
  "content_scripts":[{
	"js":["jquery.js","parse.js"],
	"css":["mockup.css"],
	"matches": ["*://www.myu.umn.edu/*"]
  }]
}
