{
	"manifest_version": 2,
  "web_accessible_resources": ["icon.png","conflict.png"],
  "name": "Rate my UMN professor",
  "version": "1.1",
  "description": "Brings up UMN Professor Ratings and informs of time conflicts",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup":"popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js","scheduleBG.js"],
    "persistent": true
  },
  "content_scripts":[{
	"js":["schedule.js","content.js"],
	"css":["mockup.css"],
	"matches": ["*://onestop2.umn.edu/courseinfo/viewSearchResults.do?campus=UMNTC*",
	"*://webapps-prd.oit.umn.edu/registration/initializeCurrentEnrollment*"
	]
  }]
}
