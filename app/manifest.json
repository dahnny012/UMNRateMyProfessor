{
	"manifest_version": 2,
  "web_accessible_resources": ["icon.png"],
  "name": "Rate my UMN professor",
  "version": "1.0",
  "description": "Bring up rate my professor ratings when choosing umn classes",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts":[{
	"js":["content.js"],
	"css":["mockup.css"],
	"matches": ["*://onestop2.umn.edu/courseinfo/viewSearchResults.do?campus=UMNTC*"]
  }]
}
