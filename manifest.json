{
	"manifest_version": 2,
  "web_accessible_resources": ["icon.png"],
  "name": "UMN Class Search",
  "version": "1.0",
  "description": "My first Chrome extension.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts":[{
	"js":["content.js"],
	"css":["mockup.css"],
	"matches": ["*://onestop2.umn.edu/*"],
	"run_at": "document_end"
  }]
}
